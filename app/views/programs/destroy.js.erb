PROGRAMS_ID = '#programs';
MODAL_ID = '#settingsModal' + "<%= j @program_id %>";

$(MODAL_ID).modal('hide');
$('body').removeClass('modal-open');
$('.modal-backdrop').remove();

$(PROGRAMS_ID).html("<%= j (render partial: 'shared/panel', collection: @programs) %>");
$(PROGRAMS_ID).append("<%= j (render partial: 'programs/new') %>");
